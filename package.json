    "terser-webpack-plugin": "^5.1.1",
    "typescript": "~3.9.3",
    "useless-files-webpack-plugin": "^1.0.1",
    "vue-template-compiler": "2.6.10",
    "webpack": "4.46.0",
    "webpack-bundle-analyzer": "^4.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
