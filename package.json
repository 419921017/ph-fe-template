    "eslint": "^6.7.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.4.1",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
